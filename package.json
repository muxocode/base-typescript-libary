{
  "name": "ts-lib",
  "version": "1.0.0",
  "private": false,
  "main": "dist/index.tsx",
  "module": "dist/index.tsx",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/muxocode/base-typescript-libary"
  },
  "scripts": {
    "test": "jest",
    "copy": "babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.*,test.*,__snapshots__",
    "delete": "node delete.js",
    "build": "npm run delete && npm run copy",
    "tojs": "tsc dist/index.ts",
    "build:js": "npm run build && npm run tojs"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "24.0.11",
    "@types/node": "11.12.0",
    "babel-cli": "^6.26.0",
    "jest": "24.7.0",
    "typescript": "3.3.4000"
  }
}
